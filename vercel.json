{
	"version": 2,
	"builds": [
		{ "src": "api/admin.py", "use": "@vercel/python" },
		{ "src": "api/chat.py", "use": "@vercel/python" },
		{ "src": "api/hello.py", "use": "@vercel/python" },
		{ "src": "api/upsert-projects.py", "use": "@vercel/python" }
	],
	"routes": [
		{ "src": "/api/admin/(.*)", "dest": "/api/admin.py" },
		{ "src": "/api/admin", "dest": "/api/admin.py" },
		{ "src": "/api/chat", "dest": "/api/chat.py" },
		{ "src": "/api/hello", "dest": "/api/hello.py" },
		{ "src": "/api/upsert-projects", "dest": "/api/upsert-projects.py" },
		{ "src": "/(.*)", "dest": "/frontend/$1" }
	]
}
